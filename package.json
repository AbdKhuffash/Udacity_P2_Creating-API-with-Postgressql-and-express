{
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
  "start": "ts-node src/server.ts",
  "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess \"node ./dist/server.js\"",
  
  
  "test:prep": "yarn migrate:test:reset && yarn migrate:test:up",
  
  "build": "tsc",
  "migrate:up": "db-migrate up",
  "migrate:down": "db-migrate down",
  "migrate:test:reset": "cross-env NODE_ENV=test db-migrate reset --env test",
  "migrate:test:up": "cross-env NODE_ENV=test db-migrate up --env test",
    "test:clean": "cross-env ENV=test NODE_ENV=test db-migrate --env test reset && cross-env ENV=test NODE_ENV=test db-migrate --env test up && yarn build",
  "test": "cross-env ENV=test NODE_ENV=test jasmine"

}
,
