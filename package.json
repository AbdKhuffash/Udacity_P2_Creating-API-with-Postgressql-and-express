    "@types/jsonwebtoken": "^9.0.10",
    "bcrypt": "^6.0.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^4.17.1",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.5.1",
    "typescript": "^5.6.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.9",
    "@types/jasmine": "^3.6.3",
    "@types/pg": "^7.14.7",
    "@types/supertest": "^6.0.3",
    "db-migrate": "^0.11.14",
    "db-migrate-pg": "^1.5.2",
    "jasmine": "^3.6.4",
    "jasmine-spec-reporter": "^6.0.0",
    "jasmine-ts": "^0.3.0",
    "supertest": "^7.1.4",
    "ts-node": "^9.1.1",
    "tsc-watch": "^7.1.1"
  }
}
